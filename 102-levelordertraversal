class Solution:
    def levelOrder(self, root: Optional[TreeNode]) -> List[List[int]]:
        if(root == None):
            return [] 
        ans = [] 
        q = deque([root]) 
        while(len(q)>0):
            level = [] 
            for i in range(len(q)):
                node = q.popleft() 
                if(node.left):
                    q.append(node.left)
                if(node.right):
                    q.append(node.right) 
                level.append(node.val) 
            ans.append(level) 
        return ans 
